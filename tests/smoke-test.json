{
  "suite": {
    "name": "Smoke Tests",
    "tests": [
      {
        "id": "TestNumberOne",
        "description": "As a customer I can log in",
        "feature": "FeatureId123",
        "ignore": false,
        "retry": 0,
        "role": "Customer",
        "goal": "LogIn",
        "setUpTasks": [
          {
            "task": "navigate",
            "preConditions": [
              { "target": "browser", "attribute": "open", "state": true }
            ],
            "postConditions": [
              { "target": "browser", "attribute": "path", "state": "/login" }
            ],
            "data": { "path": "login" }
          }
        ],
        "tearDownTasks": [],
        "testTasks": [
          {
            "task": "login",
            "preConditions": [
              { "target": "browser", "attribute": "path", "state": "/login" },
              { "target": "element", "elementReference": "app.elements.loginPage.loginButton", "attribute": "visible", "state": true }
            ],
            "postConditions": [],
            "data": {
              "loginDetails": {
                "username": "johnsmith", "password": "superman123"
              }
            }
          }
        ]
      }
    ]
  }
}